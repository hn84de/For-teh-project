function random3(){
let b = Math.random();
let a = 1;
    if(b<0.333){
            a = 1;
        }else{
            let m = 0;
        }
    
    if(b<0.666){
        if(b>0.333){
            a = 2;
        }else{
            let m = 0;
        }
    }else{
        let m = 0;
    }
    if(b<1.1){
        if(b>0.666){
            a = 3;
        }else{
            let m = 0;
        }
    }else{
        let m = 0;
    }
    return a;
}
